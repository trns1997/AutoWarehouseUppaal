chan human_finished, human_has_pod;
int taskFromQueue;
</declaration>
	<template>
		<name>TaskManager</name>
		<declaration>clock time_for_new_item;
pod_id_t list[MAX_T];
int[0,MAX_T-1] len;

// Put an element at the end of the queue
void enqueue(pod_id_t element)
{
        list[len++] = element;
}

// Remove the front element of the queue
void dequeue()
{
        int i = 0;
        len -= 1;
        while (i &lt; len)
        {
                list[i] = list[i + 1];
                i++;
        }
        list[i] = 0;
}

// Returns the front element of the queue
pod_id_t front()
{
   return list[0];
}

// Returns the last element of the queue
pod_id_t tail()
{
   return list[len - 1];
}</declaration>
