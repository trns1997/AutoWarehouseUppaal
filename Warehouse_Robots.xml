typedef int[0,numPods-1] pod_id_t;

int podPoses[numPods][2] = {
    {-3, 2},
    {-4, 2},
    {-5, 2}
};

int startingPose[2] = {0, 0};
int deliveryPose[2] = {0, 9};
int bottomLeftPose[2] = {-9, 0};
chan robot_picked[numRobots];
